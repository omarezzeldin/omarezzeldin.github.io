			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.8.1</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
					<encoding>${project.build.sourceEncoding}</encoding>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-project-info-reports-plugin</artifactId>
				<version>2.7</version>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>3.1.0</version>
				<configuration>
					<encoding>UTF-8</encoding>
					<archive>
						<addMavenDescriptor>false</addMavenDescriptor>
					</archive>
					<warSourceDirectory>${basedir}/../Model/src/com/beshara/csc/hr/emp/presentation/pages</warSourceDirectory>
					<packagingExcludes>**/business/client/**,**/business/dao/**,**/business/deploy/**,**/business/dto/**,**/business/entity/**,**/business/facade/**,**/business/integration/**,**/business/shared/**</packagingExcludes>
					<!--<warSourceExcludes>**/business/**</warSourceExcludes>-->
					<webResources>
						<resource>
							<filtering>true</filtering>
							<directory>${basedir}/../Model/src/com/beshara/csc/hr/emp/presentation</directory>
							<targetPath>WEB-INF/classes/com/beshara/csc/hr/emp/presentation</targetPath>
							<includes>
								<include>pages/**/*.jpg</include>
								<include>pages/**/*.png</include>
								<include>pages/**/*.gif</include>
								<include>pages/WEB-INF/app/config/**</include>
								<include>pages/WEB-INF/module/**</include>
								<include>pages/WEB-INF/*.xml</include>
								<include>resources/*.*</include>
							</includes>
						</resource>
						<!--resource>
							<filtering>true</filtering>
							<directory>${basedir}/../Emp-WS-proxy/src/com/beshara/csc</directory>
							<targetPath>WEB-INF/classes/com/beshara/csc</targetPath>
							<includes>
								<include>**/*.wsdl</include>
								<include>**/*.xsd</include>
							</includes>
						</resource-->
					</webResources>
					<!--webXml>${project.build.directory}/web.xml</webXml-->
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-antrun-plugin</artifactId>
				<version>3.0.0</version>
				<executions>
					<execution>
						<id>copy-war-file</id>
						<phase>package</phase>
						<goals>
							<goal>run</goal>
						</goals>
						<configuration>
							<target name="copy war file to deploy folder">
								<copy todir="deploy/">
									<fileset dir="${project.build.directory}" includes="*.war"/>
								</copy>
							</target>
						</configuration>
					</execution>
					<execution>
						<id>copy-properties-file-with-encoding</id>
						<phase>compile</phase>
						<goals>
							<goal>run</goal>
						</goals>
						<configuration>
							<encoding>${project.build.sourceEncoding}</encoding>
							<target name="copy-properties-file-with-encoding">
								<copy todir="${project.build.directory}/classes/com/beshara/csc/hr/emp/presentation/resources/" encoding="${project.build.sourceEncoding}" outputencoding="${project.build.sourceEncoding}">
									<fileset dir="${maven.multiModuleProjectDirectory}/Model/src/com/beshara/csc/hr/emp/presentation/resources/" includes="*.properties"/>
