<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config xmlns="http://java.sun.com/JSF/Configuration">
    <!-- _______________________________________________Shared Backing Beans_____________________________________-->
    <application>
        <view-handler>org.apache.myfaces.tomahawk.application.jsp.JspTilesViewHandlerImpl</view-handler>
    </application>
    <managed-bean>
        <managed-bean-name>shared_util</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.util.SharedUtilBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>sharedUtilBeanPortal</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.util.SharedUtilBeanPortal</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>integrationBean</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.util.IntegrationBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>error_page</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.backingbean.ErrorPageBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>application_util_bean</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.util.ApplicationUtilBean</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>session_clear</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.util.SessionClearBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>validation_exp</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.util.ValidationExpressionsBean</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>error_dissplay</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.util.ErrorDisplay</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>wizardbar</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.util.wizardbar.WizardBar</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>TableHeaderBean</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.util.tableheader.TableHeaderBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>treeDivBean</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.backingbean.TreeDivBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>appMainLayout</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.backingbean.AppMainLayout</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ReportsBean</managed-bean-name>
        <managed-bean-class>com.beshara.reports.presentation.backingbean.ReportsUrlsBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <!-- _________________________________ Converters Section _________________________________ -->
    <converter>
        <converter-id>SqlDateConverter</converter-id>
        <converter-class>com.beshara.jsfbase.csc.util.converters.SqlDateConverter</converter-class>
    </converter>
    <converter>
        <converter-id>MonthYearConverter</converter-id>
        <converter-class>com.beshara.jsfbase.csc.util.converters.MonthYearConverter</converter-class>
    </converter>
    <converter>
        <converter-id>TimeStampMonthYearConverter</converter-id>
        <converter-class>com.beshara.jsfbase.csc.util.converters.TimeStampMonthYearConverter</converter-class>
    </converter>
    <converter>
        <converter-id>TimeStampConverter</converter-id>
        <converter-class>com.beshara.jsfbase.csc.util.converters.TimeStampConverter</converter-class>
    </converter>
    <converter>
        <converter-id>StringConverter</converter-id>
        <converter-class>com.beshara.jsfbase.csc.util.converters.StringConverter</converter-class>
    </converter>
    <converter>
        <converter-id>LongNumberMaskConverter</converter-id>
        <converter-class>com.beshara.jsfbase.csc.util.converters.LongNumberMaskConverter</converter-class>
    </converter>
    <converter>
        <converter-id>FloatNumberMaskConverter</converter-id>
        <converter-class>com.beshara.jsfbase.csc.util.converters.FloatNumberMaskConverter</converter-class>
    </converter>
    <converter>
        <converter-id>BigDecimalNumberMaskConverter</converter-id>
        <converter-class>com.beshara.jsfbase.csc.util.converters.BigDecimalNumberMaskConverter</converter-class>
    </converter>
    <converter>
        <converter-id>DoubleThreeDecimalMaskConverter</converter-id>
        <converter-class>com.beshara.jsfbase.csc.util.converters.DoubleThreeDecimalMaskConverter</converter-class>
    </converter>
    <converter>
        <converter-id>FloatThreeDecimalMaskConverter</converter-id>
        <converter-class>com.beshara.jsfbase.csc.util.converters.FloatThreeDecimalMaskConverter</converter-class>
    </converter>
    <!-- _________________________________ End Of Converters Section _________________________________ -->
    <managed-bean>
        <managed-bean-name>OperationBar</managed-bean-name>
        <managed-bean-class>com.beshara.common.web.jsf.operationbar.OperationBarBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <lifecycle>
        <phase-listener>com.beshara.common.web.jsf.operationbar.OperationBarPhaseListener</phase-listener>
    </lifecycle>
    <managed-bean>
        <managed-bean-name>managedConstantsBean</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.util.ManagedConstantsBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ModuleMenuExt</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.util.ModuleMenuExtBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>hlpModuleCode</property-name>
            <property-class>java.lang.Long</property-class>
            <value>418</value>
        </managed-property>
        <managed-property>
            <property-name>emlModuleCode</property-name>
            <property-class>java.lang.Long</property-class>
            <value>419</value>
        </managed-property>
        <managed-property>
            <property-name>mapModuleCode</property-name>
            <property-class>java.lang.Long</property-class>
            <value>40</value>
        </managed-property>
         <managed-property>
            <property-name>hasUserGuide</property-name>
            <property-class>java.lang.Boolean</property-class>
            <value>true</value>
        </managed-property>
    </managed-bean>
    <!-- _________________________________ new wizard bar bean _________________________________ -->
    <managed-bean>
        <managed-bean-name>wizardBarBean</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.util.wizardbar2.beans.WizardBarBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <!-- notificationBean -->
    <managed-bean>
        <managed-bean-name>notificationBean</managed-bean-name>
        <managed-bean-class>com.beshara.notifications.presentation.backingbean.notifications.NotificationsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>userCode</property-name>
            <value>#{Login.user.code}</value>
        </managed-property>
    </managed-bean>
    <render-kit>
        <render-kit-id>HTML_BASIC</render-kit-id>
        <!-- ==========Overwrite The Original One, by Ashraf Gaber========== -->
        <renderer>
            <component-family>javax.faces.Panel</component-family>
            <renderer-type>org.apache.myfaces.DataScroller</renderer-type>
            <renderer-class>com.beshara.jsfbase.csc.custom.HtmlDataScrollerRenderer</renderer-class>
        </renderer>
    </render-kit>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>emailPage</from-outcome>
            <to-view-id>/app/jsps/shared/pageunderconstruction.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>emailPage</from-outcome>
            <to-view-id>/app/jsps/shared/pageunderconstruction.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addAppIssuePage</from-outcome>
            <to-view-id>/app/jsps/appissues/addappissuepage.jsp</to-view-id>
        </navigation-case>        
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>addAppProblemBean</managed-bean-name>
        <managed-bean-class>com.beshara.jsfbase.csc.backingbean.appissues.AddAppProblemBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>    
</faces-config>
